<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/german_tourism_activities"
         xmlns="http://www.semanticweb.org/german_tourism_activities#">

<owl:Ontology rdf:about="http://www.semanticweb.org/german_tourism_activities">
  <owl:imports rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Authors: Giulio Billi, Cono Cirone</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Domain: Activity recommendation and tourism in German cities.</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#hasBudget">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#Activity"/>
  <rdfs:range rdf:resource="#BudgetTier"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has budget</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting an activity to its cost classification.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasLocationSetting">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#Activity"/>
  <rdfs:range rdf:resource="#LocationSetting"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has location setting</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting an activity to its location setting (Indoor vs. Outdoor).</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isInCity">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
  <rdfs:domain rdf:resource="#Activity"/>
  <rdfs:range rdf:resource="#City"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is in city</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting an activity to the city it is in.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMeetingPoint">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
  <rdfs:domain rdf:resource="#Tour"/>
  <rdfs:range rdf:resource="#MeetingPoint"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has meeting point</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a tour to its starting location.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDuration">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTimeInterval"/>
  <rdfs:domain rdf:resource="#Tour"/>
  <rdfs:range rdf:resource="#Duration"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has duration</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a tour to its duration.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasOperatingHours">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTimeInterval"/>
  <rdfs:domain rdf:resource="#PhysicalVenue"/>
  <rdfs:range rdf:resource="#OperatingHours"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has operating hours</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a physical venue to its set of operating hours.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#appliesToDay">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#OperatingHours"/>
  <rdfs:range rdf:resource="#DayOfWeek"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">applies to day</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting an operating hours entry to a day of the week.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasLanguage">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
  <rdfs:domain rdf:resource="#Tour"/>
  <rdfs:range rdf:resource="#Language"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has language</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a tour to its language(s).</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMuseumType">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#Museum"/>
  <rdfs:range rdf:resource="#MuseumType"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has museum type</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a museum to its category.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasParkType">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#Park"/>
  <rdfs:range rdf:resource="#ParkType"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has park type</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a park to its category.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasClubType">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#NightlifeVenue"/>
  <rdfs:range rdf:resource="#ClubType"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has club type</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a nightlife venue to its category.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSightType">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <rdfs:domain rdf:resource="#Sight"/>
  <rdfs:range rdf:resource="#SightType"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has sight type</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation connecting a sight to its category.</rdfs:comment>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#opensAt">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
  <rdfs:domain rdf:resource="#OperatingHours"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">opens at</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attribute describing the opening time for an Operating Hours entry.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#closesAt">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
  <rdfs:domain rdf:resource="#OperatingHours"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">closes at</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attribute describing the closing time for an Operating Hours entry.</rdfs:comment>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Activity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A general concept for an activity of interest. Represents physical locations and events.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Activity</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#City">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalPlace"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A German city where an activity is located.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">City</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#BudgetTier">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A classification representing the cost of an Activity (Free, Low, Medium, High).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Budget Tier</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#LocationSetting">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A classification of the environment (Indoor or Outdoor).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location Setting</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#OperatingHours">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A concept to model a set of opening/closing times.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating Hours</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#DayOfWeek">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A specific day of the week.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Day of Week</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Duration">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The length of time a tour takes.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Duration</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Language">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A human language in which a tour is offered.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Language</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#MeetingPoint">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalPlace"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical location where a tour starts.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Meeting Point</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Tour">
  <rdfs:subClassOf rdf:resource="#Activity"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <owl:disjointWith rdf:resource="#PhysicalVenue"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A guided activity to visit the city. It is an event.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tour</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#PhysicalVenue">
  <rdfs:subClassOf rdf:resource="#Activity"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalPlace"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical location with set operating hours.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical Venue</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Museum">
  <rdfs:subClassOf rdf:resource="#PhysicalVenue"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An institution displaying artifacts.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Museum</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Park">
  <rdfs:subClassOf rdf:resource="#PhysicalVenue"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A public green space or natural area.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Park</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#NightlifeVenue">
  <rdfs:subClassOf rdf:resource="#PhysicalVenue"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A location for evening entertainment.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nightlife Venue</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Sight">
  <rdfs:subClassOf rdf:resource="#PhysicalVenue"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A location or structure of historical interest.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sight</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#MuseumType">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category of museum (e.g., Art, History).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Museum Type</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#ParkType">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category of park (e.g., Botanical Garden).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Park Type</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#ClubType">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category of nightlife venue (e.g., Bar, Disco).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Club Type</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#SightType">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category of sight (e.g., Monument).</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sight Type</rdfs:label>
</owl:Class>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Activity"/>
    <rdf:Description rdf:about="#City"/>
    <rdf:Description rdf:about="#BudgetTier"/>
    <rdf:Description rdf:about="#LocationSetting"/>
    <rdf:Description rdf:about="#OperatingHours"/>
    <rdf:Description rdf:about="#DayOfWeek"/>
    <rdf:Description rdf:about="#Duration"/>
    <rdf:Description rdf:about="#Language"/>
    <rdf:Description rdf:about="#MeetingPoint"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Museum"/>
    <rdf:Description rdf:about="#Park"/>
    <rdf:Description rdf:about="#NightlifeVenue"/>
    <rdf:Description rdf:about="#Sight"/>
  </owl:members>
</owl:AllDisjointClasses>


</rdf:RDF>
